<wps:Execute version="1.0.0" service="WPS" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1">
  <ows:Identifier>vires:fetch_filtered_data</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>collection_ids</ows:Identifier>
      <wps:Data>
        <wps:ComplexData mimeType="application/json">
          <![CDATA[{{{collections_ids}}}]]>
        </wps:ComplexData>
      </wps:Data>
    </wps:Input>
    {{#if variables}}
    <wps:Input>
      <ows:Identifier>variables</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{variables}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
    <wps:Input>
      <ows:Identifier>begin_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{begin_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>end_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{end_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    {{#if filters}}<wps:Input>
      <ows:Identifier>filters</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{filters}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if model_ids}}<wps:Input>
      <ows:Identifier>model_ids</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{model_ids}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if shc}}<wps:Input>
      <ows:Identifier>shc</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{shc}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if bbox}}<wps:Input>
      <ows:Identifier>bbox</ows:Identifier>
      <wps:Data>
        <wps:ComplexData>{{bbox}}</wps:ComplexData>
      </wps:Data>
    </wps:Input>{{/if}}
    {{#if time_format}}<wps:Input>
      <ows:Identifier>csv_time_format</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{time_format}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>{{/if}}
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput mimeType="{{format}}">
      <ows:Identifier>output</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>