<wps:Execute version="1.0.0" service="WPS" xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1">
  <ows:Identifier>aeolus:level2C</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>collection_ids</ows:Identifier>
      <wps:Data>
        <wps:ComplexData mimeType="application/json">
          <![CDATA[{{{collection_ids}}}]]>
        </wps:ComplexData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>begin_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{begin_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>end_time</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{end_time}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>mie_grouping_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>mie_grouping_time</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>rayleigh_grouping_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>rayleigh_grouping_end_obs</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>mie_profile_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>mie_profile_lon_of_DEM_intersection</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>rayleigh_profile_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>rayleigh_profile_lat_of_DEM_intersection,rayleigh_profile_lon_of_DEM_intersection</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>mie_wind_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>mie_wind_result_range_bin_number</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>rayleigh_wind_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>rayleigh_wind_result_range_bin_number</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>measurement_fields</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>optical_prop_crosstalk_detected</wps:LiteralData>
      </wps:Data>
    </wps:Input>

